    "compression": "^1.7.4"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "eslint": "^8.46.0",
    "prettier": "^3.0.1",
    "nodemon": "^3.0.1",
    "@types/node": "^20.5.0",
    "@types/express": "^4.17.17",
