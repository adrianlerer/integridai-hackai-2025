{
  "name": "integridai-hackai-2025",
  "version": "1.0.0-hackathon",
  "description": "IntegridAI Suite - HackAI 2025 Universidad Austral",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run api:mock\" \"npm run frontend:dev\"",
    "api:mock": "node api/mock/regtech.js",
    "frontend:dev": "cd src/frontend && npm run dev",
    "build": "npm run build:frontend && npm run build:api",
    "build:frontend": "cd src/frontend && npm run build",
    "build:api": "echo 'Mock API build complete'",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "seed:demo": "node scripts/seed-demo-data.js",
    "generate:samples": "node scripts/generate-sample-analyses.js",
    "docker:build": "docker build -t integridai-hackai .",
    "docker:run": "docker run -p 3000:3000 integridai-hackai",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod"
  },
  "keywords": [
    "regtech",
    "compliance",
    "ai",
    "hackathon",
    "argentina",
    "ley-27401",
    "fintech"
  ],
